Packages:
<table><tbody><tr>
<th>Number</th>
<th>Status</th>
<th>Last Update</th>
</tr>
<% @packages.each do |package| %>
<tr>
	<td><%= package.tracking_number -%></td>
	<td><%= package.status -%></td>
	<td><%= "#{time_ago_in_words(package.updated_at)} ago" -%></td>
</tr>
<% end %>
</tbody></table>
