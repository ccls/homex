	:id => "for_#{dom_id(@study_subject,:edit)}",
	:class => :submitter
} %>
<% end %>

<% form_for(@study_subject, 
	:url => study_subject_path(@study_subject),
	:html => { :class => 'confirm edit_study_subject' } ) do |f| %>
