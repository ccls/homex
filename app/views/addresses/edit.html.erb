<% form_for(@address,:url => address_path(@address)) do |f| %>
<%= render 'form', :f => f %>
<% end %>

<% if current_user.may_destroy? %>
<div class='dev_only'><span>Development only:</span>
<%= (@address.new_record?)? "&nbsp" : 
destroy_link_to( "Destroy", address_path(@address) )
%>
</div>
<% end %>
