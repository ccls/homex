<% content_tag_for( :tr, enrollment, :class => 'row' ) do %>
<td class='project'>
<%= link_to enrollment.project.code, 
	enrollment_path(enrollment) %>
</td>
<td class='candidate'>
<%=yndk enrollment.is_candidate %>
</td>
<td class='locate'>
<%#=yndk enrollment.able_to_locate %>
<%= enrollment.tracing_status || '&nbsp;' %>
</td>
<td class='eligible'>
<%=yndk enrollment.is_eligible %>
</td>
<td class='consented'>
<%=mdy enrollment.consented_on %>
</td>
<td class='completed'>
<%=mdy enrollment.completed_on %>
</td>
<% end %>
