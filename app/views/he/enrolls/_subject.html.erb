<% content_tag_for( :div, subject, :class => 'row' ) do %>
<div class='childid'>
<%= link_to subject.childid, he_subject_path(subject) -%></div>
<div class='studyid'>
<%= subject.studyid %>
</div>
<div class='priority'>
<%= subject.priority %>
</div>
<div class='last_name'>
<%= subject.last_name %>
</div>
<div class='first_name'>
<%= subject.first_name %>
</div>
<div class='outcome'>
<%= subject.outcome %>
</div>
<div class='outcome_date'>
<%= subject.outcome_date %>
</div>
<% end %><%# content_tag_for %>
